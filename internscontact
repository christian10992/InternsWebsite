<!--Dropdown menu for mobile-->
            <script>
                function dropdownMenu() {
                var x = document.getElementById("dropdownClick");
                if (x.className === "navbar") {
                    x.className += " responsive";
                    /*change navbar to navbar.responsive*/
            } else {
                x.className = "navbar";
            }
            }
            </script>
